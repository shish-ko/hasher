{
  "name": "secret_service",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm ./server ",
    "dev:no-token": "cross-env VITE_AUTH_FREE=true ts-node --esm ./server",
    "build": "npm run build:client && npm run build:server",
